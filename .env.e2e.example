# E2E Test Environment Variables
# Copy this file to .env.e2e and fill in your actual values
# DO NOT commit .env.e2e to version control

# SAP Integration Suite Configuration
# Base URL of your SAP Integration Suite API endpoint
SAP_BASE_URL=https://your-tenant.integrationsuitetrial-api.eu10.hana.ondemand.com/api/v1

# OAuth Configuration for SAP Integration Suite
# Client ID for OAuth authentication
SAP_OAUTH_CLIENT_ID=your-client-id

# Client Secret for OAuth authentication
SAP_OAUTH_CLIENT_SECRET=your-client-secret

# OAuth Token URL for authentication
SAP_OAUTH_TOKEN_URL=https://your-tenant.authentication.eu10.hana.ondemand.com/oauth/token

# Redis Configuration (optional, for cache testing)
# Connection string for Redis instance (e.g., redis://localhost:6379)
REDIS_CONNECTION_STRING=redis://localhost:6379

# Enable/disable Redis for E2E tests
# Set to 'true' to enable Redis caching, 'false' to disable
REDIS_ENABLED=true

# E2E Test Data Configuration (optional)
# These can be used to specify test artifacts, packages, or other test data
# TEST_ARTIFACT_ID=your-test-artifact-id
# TEST_PACKAGE_ID=your-test-package-id
# TEST_USER_INFO=your-test-user-info
