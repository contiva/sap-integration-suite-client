{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "theme": "default",
  "name": "SAP Integration Suite Client - API Documentation",
  "includeVersion": true,
  "readme": "README.md",
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "sort": ["source-order"],
  "plugin": [],
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/tests/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "excludeExternals": true,
  "githubPages": true,
  "hideGenerator": false,
  "searchInComments": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "logLevel": "Info",
  "emit": "docs",
  "compilerOptions": {
    "skipLibCheck": true
  }
}

